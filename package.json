{
  "name": "b2b-application",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve-dev": "ng serve --configuration=dev",
    "serve-test": "ng serve --configuration=test",
    "serve-prod": "ng serve --configuration=production",
    "test-script": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "build-dev": "ng build --configuration=dev",
    "build-dev-with-tests": "npm run test-script && ng build --configuration=dev",
    "build-test": "ng build --configuration=test",
    "build-test-with-tests": "npm run test-script && ng build --configuration=test",
    "build-prod": "ng build --configuration=production",
    "build-prod-with-tests": "npm run test-script && ng build --configuration=production",
    "docker-create-image": "docker build -t image-egemen-ceylan .",
    "docker-run-image": "docker run -p 4201:80 --name egemen-ceylan image-egemen-ceylan",
    "docker-serve": "npm run docker-create-image && npm run docker-run-image",
    "docker-serve-with-tests": "npm run test-script && npm run docker-create-image && npm run docker-run-image"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@ngrx/store": "^9.2.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
